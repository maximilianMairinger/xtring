{
  "name": "xtring",
  "version": "1.2.1",
  "description": "String prototype enhancements.",
  "main": "./app/dist/esm/xtring.js",
  "types": "xtring.d.ts",
  "exports": {
    "node": {
      "import": "./app/dist/esm/xtring.js",
      "require": "./app/dist/cjs/xtring.js"
    },
    "default": "./app/dist/esm/xtring.js"
  },
  "scripts": {
    "build": "npm run buildESM; npm run buildCJS",
    "buildESM": "tsc -p ./tsconfig.prod.esm.json",
    "buildCJS": "tsc -p ./tsconfig.prod.cjs.json",
    "dev": "npm run devWeb",
    "devWeb": "rollup --config rollup.web.dev.config.js -w",
    "devServer": "rollup --config rollup.server.dev.config.js -w",
    "deploy": "npm run build && npm publish",
    "test": "node ./test/dist/test-bundle",
    "repl": "node repl/dist/xtring"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianMairinger/xtring.git"
  },
  "keywords": [
    "string",
    "prototype",
    "enhancement",
    "util"
  ],
  "author": "Maximilian Mairinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximilianMairinger/xtring/issues"
  },
  "homepage": "https://github.com/maximilianMairinger/xtring#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.0",
    "@types/node": "^13.13.4",
    "rollup": "^2.18.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "attatch-to-prototype": "^1.3.1"
  }
}
